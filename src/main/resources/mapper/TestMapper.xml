<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.humy.executor.mapper.TestMapper">


    <select id="queryStudentById" resultType="com.humy.executor.Entity.Student">
        select * from student where id = "1"
    </select>


    <select id="queryTeacherById" resultType="com.humy.executor.Entity.Teacher">
        select * from teacher where id = "1"
    </select>

</mapper>
